<!DOCTYPE html>
<html lang="en">
<head>
    {{>headerLinks}}
</head>
<body>
    {{>navbar}}
    <div class="containter-fluid main_header">
        <div class="row">
            <div class="col-md-10 col-12 mx-auto">
                <div class="main_content">
                    <form class="temp_form">
                        <input type="text" id="cityName" placeholder="Enter  your  City  Name" autocomplete="off">
                        <input type="submit" value="Search" id="submitBtn">
                    </form>

                </div>
                <div class="temp_info">
                    <div class="top_layer">
                        <p id="day">Tuesday</p>
                        <p id="today_date">14 May</p>
                    </div>
                    <div class="main_layer">
                        <p id="city_name">Get Output Here</p>
                        <div class="middle_layer">
                            <p class="temp"><span class="temp_real_val">0</span><sup>0</sup>C</p>
                            <p id="temp_status"></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{>footer}}
    <script src="js/main.js">
    </script>
</body>
</html>